{
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true,
    "eslint.validate": [
      "javascript",
      "javascriptreact",
      "typescript",
      "typescriptreact"
    ],
    "eslint.options": {
      "extensions": [".js", ".jsx", ".ts", ".tsx"]
    },
    "files.autoSave": "onFocusChange",
    "files.associations": {
      "*.ts": "typescript",
      "*.tsx": "typescriptreact"
    }
  }
  